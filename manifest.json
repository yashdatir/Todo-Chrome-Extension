{
  "name": "TODO:Temp Storage",
  "description": "This extension is used to temporarily store data to a todo list using localstorage.",
  "version": "0.2",
  "manifest_version": 3,
  "permissions": ["storage", "scripting"],
  "action": {
    "default_popup": "index.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "16.png",
    "48": "48.png",
    "128": "128.png"
  }
}
